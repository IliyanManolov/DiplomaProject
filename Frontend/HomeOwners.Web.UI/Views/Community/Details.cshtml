@model CommunityDetailsViewModel;

<div class="dashboard-container d-flex justify-content-center align-items-start gap-5 p-4">

    <div class="card-column d-flex flex-column gap-4" style="max-height: 80vh; width:30vw; overflow-y: auto;">
        <partial name="_MeetingsDisplay" model="@Model.CommunityMeetingViewModels"></partial>
    </div>

    <div class="message-feed" style="width: 50vw; max-height: 80vh; overflow-y: auto;">
        <partial name="_MessagesDisplay" model="@Model.CommunityMessageViewModels"></partial>
    </div>

    <div class="card-column d-flex flex-column gap-4" style="max-height: 80vh; width:20vw; overflow-y: auto;">
        <partial name="_PropertiesShortDisplay" model="@Model.PropertiesViewModels"></partial>
    </div>
</div>